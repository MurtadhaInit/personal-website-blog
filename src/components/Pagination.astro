---
// component imports
import Link from "@components/Link.astro";

const { prevURL, nextURL } = Astro.props;
---

<nav>
  {
    prevURL && (
      <Link
        text="Previous"
        href={prevURL}
        style="primary"
        borderVisible={true}
        isFilled={false}
        icon={{
          name: "tabler:arrow-big-left-line",
          side: "left",
        }}
      />
    )
  }
  {
    nextURL && (
      <Link
        text="Next"
        href={nextURL}
        style="primary"
        borderVisible={true}
        isFilled={false}
        icon={{
          name: "tabler:arrow-big-right-line",
          side: "right",
        }}
      />
    )
  }
</nav>
